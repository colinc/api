      },
      "post" :{
        "description": "",
        "tags": [
          "Meeting"
        ],
        "operationId": "meetingRegCreate",
        "parameters" : [
          {
            "in": "path",
            "name": "number",
            "type": "integer",
            "required": true
          },
          {
            "in": "query",
            "name": "occurence_id",
            "type": "string",
            "required": true
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/users": {
      "get" : {
        "description": "List all Users",
        "tags": [
          "User"
        ],
        "operationId": "userList",
        "parameters": [
          {
            "in": "query",
            "name": "status",
            "type": "string",
            "default": "active",
            "enum":["active","inactive","pending"]
          },
          {
            "in": "query",
            "name": "page_size",
            "type": "integer"
          },
          {
            "in": "query",
            "name": "page_number",
            "type": "integer"
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      },
      "post": {
        "description": "Create a user on Zoom, returns the created user",
        "tags": [
          "User"
        ],
        "operationId": "userCreate",
        "parameters": [
          {
            "in": "body",
            "name": "user",
            "required": true,
            "description": "User",
            "schema": {
              "type": "object",
              "properties": {
                "action": {
                  "title": "action",
                  "description": "Create User Type",
                  "type":"string",
                  "enum":[
                    "create",
                    "autoCreate",
                    "ssocreate",
                    "custcreate"
                  ]
                },
                "user_info": {
                  "type":"object",
                  "properties":{
                    "email":{
                      "type": "string",
                      "format": "email"
                    },
                    "type":{
                      "type": "integer",
                      "enum":[0,1,2]
                    },
                    "first_name":{
                      "type": "string"
                    },
                    "last_name":{
                      "type": "string"
                    },
                    "password":{
                      "type": "string",
                      "format": "password"
                    }
                  }
                }
              },
              "required": ["action", "user_info"]
            }
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/users/{id}" : {
      "get" : {
        "description": "Get a user",
        "tags": [
          "User"
        ],
        "operationId": "userGet",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "required": true
          },
          {
            "in": "query",
            "name": "login_type",
            "type": "integer"
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      },
      "put" :{
        "description": "Update a user",
        "tags": [
          "User"
        ],
        "operationId": "userUpdate",
        "parameters": [
          {
            "name": "id",
            "description" : "User ID",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "in": "body",
            "name": "body",
            "required": true,
            "description": "User",
            "schema": {
              "type":"object",
              "properties":{
                "first_name":{
                  "type": "string"
                },
                "last_name":{
                  "type": "string"
                },
                "type":{
                  "type": "integer",
                  "enum":[0,1,2]
                },
                "pmi":{
                  "type": "string"
                },
                "timezone":{
                  "type": "string",
                  "format": "date-time"
                },
                "dept":{
                  "type": "string"
                },
                "vanity_name":{
                  "type": "string"
                },
                "host_key":{
                  "type": "integer"
                }
              }
            }
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      },
      "delete" : {
        "description": "Delete a user on Zoom",
        "tags": [
          "User"
        ],
        "operationId": "userDelete",
        "parameters": [
          {
            "name": "id",
            "description" : "User ID",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "name": "action",
            "description" : "Delete Action Type<br/>**disassociate**: Dissassociate a User from account<br/>**delete**: Permanently Delete a user",
            "in": "query",
            "type": "string",
            "enum" : [
              "disassociate",
              "delete"
            ]
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/users/{id}/assistants" : {
      "get" : {
        "description": "List user's assistants",
        "tags": [
