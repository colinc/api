  "paths": {
    "/meetings/{number}" :{
      "get" :{
        "description": "Get a meeting",
        "tags": [
          "Meeting"
        ],
        "operationId": "meetingGet",
        "parameters" : [
          {
            "in": "path",
            "name": "number",
            "type": "integer",
            "required": true
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      },
      "put" :{
        "description": "Update a meeting",
        "tags": [
          "Meeting"
        ],
        "operationId": "meetingUpdate",
        "parameters" : [
          {
            "in": "path",
            "name": "number",
            "type": "integer",
            "required": true
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      },
      "delete" :{
        "description": "Delete a meeting",
        "tags": [
          "Meeting"
        ],
        "operationId": "meetingDelete",
        "parameters" : [
          {
            "in": "path",
            "name": "number",
            "type": "integer",
            "required": true
          },
          {
            "in": "query",
            "name": "occurence_id",
            "type": "string",
            "required": true
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/meetings/{number}/registrants" :{
