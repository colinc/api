          {
            "name": "id",
            "description" : "User ID",
            "in": "path",
            "type": "string",
            "required" : true
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/users/{id}/password" :{
      "put":{
        "description" : "Update a user's password",
        "tags": [
          "User"
        ],
        "operationId": "userPassword",
        "parameters" :[
          {
            "name": "id",
            "description" : "User ID",
            "in": "path",
            "type": "string",
            "required" : true
          },
          {
            "name": "body",
            "description" : "User password",
            "in": "body",
            "schema" : {
              "type" : "object",
              "properties" : {
                "password" :{
                  "type" :"string",
                  "format": "password"
                }
              }
            }
          }
        ],
        "responses":{
          "200": {
            "description":"Success"
          }
        }
      }
    },
    "/users/{id}/picture" :{
      "put":{
        "description" : "update a user's picture",
        "tags": [
          "User"
        ],
        "operationId": "userPicture",
        "parameters" :[
          {
            "name": "id",
            "description" : "User ID",
            "in": "path",
            "type": "string",
            "required" : true
          },
          {
            "name": "body",
            "description" : "User picture file, must be a jpg/jpeg file",
            "in": "body",
            "schema" : {
              "type" : "object",
              "properties" : {
                "pic_file" :{
                  "type" :"string",
