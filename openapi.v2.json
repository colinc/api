          },
          {
            "name": "body",
            "description" : "User assistant",
            "in": "body",
            "schema" : {
              "type" : "object",
              "properties" : {
                "email_or_id" : {
                  "type" : "string"
                }
              }
            }
          }
        ],
        "responses":{
